{
	"name": "@krowten/svelte-heroicons",
	"version": "0.0.5",
	"description": "Heroicons for Svelte",
	"repository": {
		"type": "git",
		"url": "https://github.com/krowten/svelte-heroicons"
	},
	"engines": {
		"node": ">=14.0.0"
	},
	"author": "Vladimir <vladimir@kraykovskiy.ru>",
	"license": "MIT",
	"keywords": [
		"svelte",
		"sveltekit",
		"tailwindcss",
		"icon",
		"icons",
		"heroicons"
	],
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "rm -rf build && svelte-kit build",
		"publish": "npm publish ./package",
		"package": "svelte-kit package",
		"deploy": "npm run build && gh-pages -d build -t true",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"postinstall": "node ./scripts/build.js"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-static": "^1.0.0-next.26",
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"autoprefixer": "^10.4.2",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"gh-pages": "^3.2.3",
		"postcss": "^8.4.5",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.9.4",
		"svelte2tsx": "^0.4.14",
		"tailwindcss": "^3.0.15",
		"ts-node": "^10.4.0",
		"tslib": "^2.3.1",
		"typescript": "^4.5.4"
	},
	"type": "module",
	"dependencies": {
		"heroicons": "^1.0.6"
	}
}
